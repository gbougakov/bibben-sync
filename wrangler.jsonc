{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "bibben-sync",
	"main": "src/index.ts",
	"compatibility_date": "2026-01-03",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"hyperdrive": [
		{
			"binding": "HYPERDRIVE",
			"id": "955a14f0b3e44f689bdc198b65060ca1",
			"localConnectionString": "postgresql://postgres:password@localhost:5432/bibben"
		}
	],
	"triggers": {
		"crons": ["35 * * * *", "0 3 * * *"]
	}
}
